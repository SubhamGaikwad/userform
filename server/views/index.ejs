<!DOCTYPE html>
<html>
<head>
  <title>List of Users</title>
  <style>
    table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
      padding: 5px;
    }
  </style>
</head>
<body>
  <h1>List of Users</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Age</th>
      <th>City</th>
      <th>Profession</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= user.city %></td>
        <td><%= user.profession %></td>
      </tr>
    <% }); %>
  </table>
  <br>
  <a href="/user/add">Add User</a>
</body>
</html>
